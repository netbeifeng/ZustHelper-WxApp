/*字体开始*/

@font-face {
  font-family: 'icomoon';
  src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'), url('icomoon.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"],
[class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-close:before {
  content: "\e900";
}

.icon-silence:before {
  content: "\e901";
}

.icon-setting:before {
  content: "\e902";
}

.icon-sound:before {
  content: "\e903";
}


/*字体结束*/

.container {
  box-sizing: border-box;
  position: absolute;
  padding: 10px;
  padding-top: 0;
  width: 100%;
  height: 100%;
  background: #F8F1E9;
  overflow: hidden;
}

.container .score-btns {
  box-sizing: border-box;
  width: 100%;
  height: 140px;
  color: #fff;
  margin-top: 30rpx;
}

.container .score-btns .score {
  float: left;
  margin-right: 10px;
  height: 140px;
  background: #EDC818;
  border-radius: 4px;
}

.score-btns .score .label {
  display: block;
  width: 100%;
  height: 52px;
  line-height: 52px;
  text-align: center;
  font-size: 28px;
}

.score-btns .score .value {
  display: block;
  width: 100%;
  height: 88px;
  line-height: 88px;
  text-align: center;
  font-size: 54px;
}

.container .score-btns .btns {
  box-sizing: border-box;
  float: left;
  padding: 10px;
  width: 140px;
  height: 140px;
  border-radius: 4px;
  background: #D8D8D8;
}

.score-btns .btns .icon {
  display: block;
  float: left;
  width: 55px;
  height: 55px;
  line-height: 55px;
  text-align: center;
  border-radius: 50%;
  background: #B5B5B5;
  font-size: 24px;
  font-weight: bold;
}

.score-btns .btns .icon.left {
  margin-right: 10px;
}

.score-btns .btns .rounds {
  height: 55px;
}

.score-btns .btns .btn-reset {
  margin-top: 10px;
  width: 100%;
  height: 55px;
  line-height: 55px;
  background: #F57900;
  color: #fff;
}

.game-wrap {
  box-sizing: border-box;
  position: relative;
  margin: auto;
  margin-top: 10px;
  background: #B9AD9F;
  border: 2px solid #B9AD9F;
  border-radius: 4px;
}

.game-wrap .game-container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.game-container .cell-grid {
  position: absolute;
  float: left;
  display: block;
  background: #B9AD9F;
  border: 2px solid #B9AD9F;
  box-sizing: border-box;
}

.game-container .cell-grid .cell-wrap {
  position: relative;
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 8px;
  background: #CBBFB1;
}

.cell-wrap .cell {
  position: absolute;
  z-index: 1;
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 8px;
}

.cell-wrap .cell >.number {
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -15px;
  width: 100%;
  height: 30px;
  font-size: 18px;
  line-height: 30px;
  text-align: center;
  color: #000;
}


/*弹出层*/

.popout .mask {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  height: 100%;
  background: #000000;
  opacity: 0.4;
}

.popout .content {
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 991;
  transform: translate(-50%, -50%);
  padding: 20px;
  width: 280px;
  height: 320px;
  background: #fff;
  border-radius: 2px;
}

.popout .content .icon-close {
  position: absolute;
  top: -10px;
  right: -10px;
  width: 30px;
  height: 30px;
  line-height: 30px;
  color: #999;
  font-weight: bold;
  text-align: center;
  border-radius: 50%;
  background: #fff;
}

.popout .content button {
  margin-top: 20px;
  background: #FF9B3E;
  color: #fff;
}


